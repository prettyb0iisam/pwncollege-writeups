          'mov rax, 0x00'
          'mov [rsi], rax'
          'mov [rsi+1], rax'
          'mov rax, 0xee'
          'mov [rsi+2], rax'
          'mov rax, 0xff'
          'mov [rsi+3], rax'
          'mov rax, 0xc0'
